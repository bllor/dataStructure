package ch02;

public class practice_2_7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 100이하의 소수 출력하기
		
		int num = 10;
		
		for(int i = 2; i <=num;i++) {
			for(int j = 2;j<i;j++) {
				if(i%j==0) {
					System.out.println(i);
				}else {
					System.out.println("prime :"+i);
				}
			}
		}
		
	}

}
